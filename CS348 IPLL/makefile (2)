#declare variable
CC=gcc
FC=flex

CFLAG=-c -Wall
CFLAGW=-Wall

#Commmands
output.txt: a.out A3_28.c
	./a.out < A3_28.nc > output.txt

a.out: lex.yy.c A3_28.o 
	$(CC) $(CFLAGW) A3_28.o -lfl

lex.yy.c: A3_28.l
	$(FC) A3_28.l

main.o: A3_28.c
	$(CC) $(CFLAG) A3_28.c 

clean:
	rm A3_28.c output.txt lex.yy.c a.out *.o